<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r9kyqjp5t2yw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:background="@drawable/cr40b090a13">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/rjivl92wapa_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="46dp"
                >
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rjivl92wapa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bgimage"
                    android:scaleType="fitXY"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/rq7t02ttwalq"
                        app:shapeAppearanceOverlay="@style/cr40"
                        android:layout_width="69dp"
                        android:layout_height="69dp"
                        android:layout_marginTop="45dp"
                        android:layout_marginBottom="15dp"
                        android:layout_marginStart="20dp"
                        android:src="@drawable/back"
                        android:scaleType="fitXY"
                        />
                    <LinearLayout
                        android:id="@+id/r9v78suosyv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:layout_marginBottom="185dp"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/rndysueik9qs"
                            android:textColor="#CDDEFF"
                            android:textSize="28sp"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Bắt đầu"
                            />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/rzdp8dvyier"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="204dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rq8m1chx7akp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/cr10b4267b2"
                    android:paddingVertical="14dp"
                    android:layout_marginBottom="24dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/r72ooyzbymkd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/r2gjkch4ljmc"
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:layout_marginEnd="4dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/r9ne7oei56jl"
                            android:textColor="#FFFFFF"
                            android:textSize="17sp"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Facebook"
                            />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/rah1z6ctyps5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/cr10bea4335"
                    android:paddingVertical="14dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/r3cdjkrr6t47"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/rhdt8zx0hm4"
                            android:layout_width="22dp"
                            android:layout_height="22dp"
                            android:layout_marginEnd="4dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"
                            />
                        <TextView
                            android:id="@+id/r8znajs3dhgk"
                            android:textColor="#FFFFFF"
                            android:textSize="17sp"
                            android:textStyle="bold"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Google"
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:id="@+id/rvv9tqchb71"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Bạn đã có tài khoản? Đăng nhập"
                    android:textColor="#2C58AF"
                    android:textSize="15sp"/>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/rgijk6x1lbug"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="#111326"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/rncjkvw2ti8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="41dp"
                    android:text="Tạo tài khoản"
                    android:textColor="#FEFEFE"
                    android:textSize="17sp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
