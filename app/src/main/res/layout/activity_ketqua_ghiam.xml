<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r2qsgzoz2xpg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/cr40bf5ede0">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rtm7otf0w89"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="19dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="9dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rs33jyg687i7"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/avatar"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/re5hotrew8p"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/chuong"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r934j3xzxyol"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="87dp"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="11dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/ra0rvj63dtgm"
                    app:shapeAppearanceOverlay="@style/cr40"
                    android:layout_width="69dp"
                    android:layout_height="69dp"
                    android:src="@drawable/back"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rfdibxwck8gw"
                    app:shapeAppearanceOverlay="@style/cr40"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/Menu"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/rc15o7h3asfj"
                android:textColor="#4A4A4A"
                android:textSize="32sp"
                android:layout_marginBottom="88dp"
                android:layout_marginStart="27dp"
                android:layout_height="wrap_content"
                android:layout_width="314dp"
                android:text="Kết quả đánh giá bằng\nghi âm"
                />
            <TextView
                android:id="@+id/rv1zqk5s6fnj"
                android:textColor="#4A4A4A"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="31dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Trầm cảm trung bình"
                />
            <TextView
                android:id="@+id/rlqh8l0h5cea"
                android:textColor="#4A4A4A"
                android:textSize="16sp"
                android:layout_marginBottom="159dp"
                android:layout_marginStart="31dp"
                android:layout_height="wrap_content"
                android:layout_width="293dp"
                android:text="Bạn nên trò chuyện với bác sĩ hoặc chuyên gia sức khoẻ tâm thần"
                />
            <TextView
                android:id="@+id/rezpp5ej7fpg"
                android:textColor="#C28989"
                android:textSize="16sp"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="48dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Xem đề xuất bộ âm nhạc dành cho bạn"
                />
            <LinearLayout
                android:id="@+id/rz4r29beefv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/cr20b7fa7b5"
                android:paddingVertical="13dp"
                android:layout_marginBottom="74dp"
                android:layout_marginHorizontal="43dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rafcv6itkb1"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Kết thúc"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
