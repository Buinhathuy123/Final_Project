<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rgskdb8ru1sg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/cr40bf5ede0">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rci7iah53sdo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="19dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="9dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ry180wze2yl"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/avatar"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/rf3ouh89ln34"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/chuong"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r5ghgjed8u94"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="95dp"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="11dp"
                android:orientation="horizontal">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/r99b066ir108"
                    app:shapeAppearanceOverlay="@style/cr40"
                    android:layout_width="69dp"
                    android:layout_height="69dp"
                    android:src="@drawable/back"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rtp2vhz88lo"
                    app:shapeAppearanceOverlay="@style/cr40"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/Menu"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/rnhzr1j5nchl"
                android:textColor="#4A4A4A"
                android:textSize="32sp"
                android:layout_marginBottom="80dp"
                android:layout_marginStart="31dp"
                android:layout_height="wrap_content"
                android:layout_width="313dp"
                android:text="Kết quả đánh giá bằng\nhình ảnh"
                />
            <TextView
                android:id="@+id/rxkmwbhgwgz"
                android:textColor="#4A4A4A"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="31dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Trầm cảm trung bình"
                />
            <TextView
                android:id="@+id/rrk64zpie7uj"
                android:textColor="#4A4A4A"
                android:textSize="16sp"
                android:layout_marginBottom="159dp"
                android:layout_marginStart="31dp"
                android:layout_height="wrap_content"
                android:layout_width="293dp"
                android:text="Bạn nên trò chuyện với bác sĩ hoặc chuyên gia sức khoẻ tâm thần"
                />
            <TextView
                android:id="@+id/r2ttsw0qj99o"
                android:textColor="#C28989"
                android:textSize="16sp"
                android:layout_marginBottom="40dp"
                android:layout_marginStart="48dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Xem đề xuất bộ âm nhạc dành cho bạn"
                />
            <LinearLayout
                android:id="@+id/raf9m3etc9f6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/cr20b7fa7b5"
                android:paddingVertical="13dp"
                android:layout_marginBottom="74dp"
                android:layout_marginHorizontal="43dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r88l0y11onat"
                    android:textColor="#4A4A4A"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Kết thúc"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
