<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rr3w167obsf"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/cr40bf5ede0">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ry7xkxecans"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="19dp"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="9dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rgh1jq11ze3v"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/avatar"
                    android:scaleType="fitXY"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/re0re7b0jxdr"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:src="@drawable/chuong"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rr9s5581nv4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_marginBottom="84dp"
                android:orientation="vertical">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/r0uekgfe0hw6"
                    app:shapeAppearanceOverlay="@style/cr40"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginEnd="11dp"
                    android:src="@drawable/menu"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/r1g5xgm233jj"
                android:textColor="#4A4A4A"
                android:textSize="36sp"
                android:layout_marginBottom="49dp"
                android:layout_marginStart="48dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Lịch sử đánh giá"
                />
            <LinearLayout
                android:id="@+id/rzrffiprhd5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/sffffffsw1cr10b7fa7b5"
                android:paddingVertical="14dp"
                android:paddingLeft="11dp"
                android:layout_marginBottom="23dp"
                android:layout_marginHorizontal="41dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ra01frrdthhu"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Lịch sử đánh giá hình ảnh"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rkrxwp5lrccp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/sffffffsw1cr10b7fa7b5"
                android:paddingVertical="13dp"
                android:paddingLeft="11dp"
                android:layout_marginBottom="23dp"
                android:layout_marginHorizontal="41dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rmlqlnc7z9i"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Lịch sử đánh giá trắc nghiệm"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rz06egla0zu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/sffffffsw1cr10b7fa7b5"
                android:paddingVertical="14dp"
                android:paddingLeft="11dp"
                android:layout_marginBottom="329dp"
                android:layout_marginHorizontal="41dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r4sqktjlr3eb"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Lịch sử đánh giá ghi âm"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
